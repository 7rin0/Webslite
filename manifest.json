    {
      "manifest_version": 2,

      "name": "Webslite Premium",
      "description": "Webslite - Turn Website into Webslide",
      "version": "5.0",

      "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "webslite.html"
      },

        /**
      "page_action": {
        "default_icon": { "38": "icon38.png"},
        "default_popup": "webslite.html",
        "default_title": "Webslite Premium 5.0"
      },**/

     "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "126": "icon126.png"
      },

      "content_scripts": [ {
          "matches": [ "http://*/*", "https://*/*" ],
          "js": [ "jquery-2.1.1.min.js", "fotorama.js", "webslite.js"],
          "css": [ "fotorama.css"]

      }],

      "permissions": [
        "http://*/*",
        "contentSettings"
      ]
    }
